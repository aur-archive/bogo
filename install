pre_upgrade () {
	cd /usr/share
	if [ -d ibus-bogo ]; then
		rm -rf ibus-bogo/*
	fi
	
	cd /usr/share/ibus/component/
	if [ -f bogo.xml ]; then
		rm bogo.xml
	fi
	
	cd /usr/lib/python2.7/site-packages/
	if [ -d BoGo ]; then
		rm -rf BoGo/*
	fi
	
	local lib=()
	for i in `ls /usr/lib | grep bogo`; do
		rm -rf /usr/lib/$i
	done
	}

		
